<h1>Sessions#new</h1>
<p>Please Sign In</p>

<%= form_tag('/signin', method: 'post') do %>
<label>User Name:</label>
<%= select_tag "user[name]", options_from_collection_for_select(User.all, "name", "name")%>
<label>Password:</label>
<%= password_field_tag 'password' %>
<%= submit_tag 'Sign In' %>
<% end %>