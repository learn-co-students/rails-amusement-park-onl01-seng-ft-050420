<h1>Sign In</h1>



<%= form_tag('/create', method: :post) do %>
    <%= label_tag :user_name, "Username:" %>
    <%= select_tag("user_name", options_for_select(@users.collect {|u| u.name })) %><br>
    
    <%= label_tag :password, "Password:" %>
    <%= password_field_tag :password %><br>
    <%= submit_tag "Sign In"%>
<% end %>

